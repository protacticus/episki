{% extends 'admin/layout.html.twig' %}

{% block body_id 'artifact_new' %}

{% block main %}
    <h1>{{ 'title.artifact_new'|trans }}</h1>

    {{ form_start(form) }}
        {{ form_row(form.file) }}


        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'label.create_artifact'|trans }}
        </button>
        <a href="{{ path('artifact_index') }}" class="btn btn-link">
            <i class="fa fa-list-alt" aria-hidden="true"></i> {{ 'action.back_to_list'|trans }}
        </a>
    {{ form_end(form) }}
{% endblock %}

{% block sidebar %}
    {{ parent() }}


{% endblock %}

